@model SGIAMT.Models.ENivelxTipoNivel

@{
    ViewData["Title"] = "Create";
}

    <h1>Registrar los niveles del alumno </h1>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="PkIntnCod" class="control-label"></label>
                <input asp-for="PkIntnCod" class="form-control" value="1" />
                <span asp-validation-for="PkIntnCod" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FkInCod" class="control-label"></label>
                <select asp-for="FkInCod" class="form-control" asp-items="ViewBag.FkInCod">

                </select>
            </div>
            <div class="form-group">
                <label asp-for="FkItnCod" class="control-label"></label>
                <select asp-for="FkItnCod" class="form-control" asp-items="ViewBag.FkItnCod">

                </select>
            </div>
            <div class="form-group">
                <label asp-for="NroAlumno" class="control-label"></label>
                <input asp-for="NroAlumno" class="form-control" value="1" />
                <span asp-validation-for="NroAlumno" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="FkIuDni" class="control-label"></label>
                <select asp-for="FkIuDni" class="form-control" asp-items="ViewBag.FkIuDni"></select>
            </div>
            <div class="form-group">
                <input type="submit" value="Registrar Niveles" class="btn btn-success" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
